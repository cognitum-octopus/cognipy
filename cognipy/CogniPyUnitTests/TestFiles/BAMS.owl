<oboInOwl:hasDefinition>
<oboInOwl:Definition>
<rdfs:label xml:lang="en">As for has_participant, but with the additional condition that the component instance is causally active in the relevant process</rdfs:label>
<oboInOwl:hasDbXref>
<oboInOwl:DbXref>
<rdfs:label>PMID:15892874</rdfs:label>
<oboInOwl:hasURI rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/obo/owl/PMID#PMID_15892874</oboInOwl:hasURI>
</oboInOwl:DbXref>
</oboInOwl:hasDbXref>
</oboInOwl:Definition>
</oboInOwl:hasDefinition>
</owl:ObjectProperty>
