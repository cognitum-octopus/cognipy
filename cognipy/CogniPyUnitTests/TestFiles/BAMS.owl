<owl:Class rdf:about="http://brancusi.usc.edu/bkms/BAMS.owl#BAMSC43">
<rdfs:label>Taenia tecta</rdfs:label>
<rdfs:subClassOf>
<owl:Restriction>
<owl:onProperty rdf:resource="http://brancusi.usc.edu/bkms/BAMS.owl#ro:has_part"/>
