<!--  http://brancusi.usc.edu/bkms/BAMS.owl#BAMSC48  -->
<owl:Class rdf:about="http://brancusi.usc.edu/bkms/BAMS.owl#BAMSC48">
<rdfs:label>Taenia tecta dorsal part layer 4</rdfs:label>
<rdfs:subClassOf>
<owl:Restriction>
