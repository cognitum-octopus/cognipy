<!--  http://brancusi.usc.edu/bkms/BAMS.owl#BAMSC212  -->
<owl:Class rdf:about="http://brancusi.usc.edu/bkms/BAMS.owl#BAMSC212">
<rdfs:label>Central nucleus of amygdala</rdfs:label>
<rdfs:subClassOf>
<owl:Restriction>
