<!--  http://brancusi.usc.edu/bkms/BAMS.owl#BAMSC588  -->
<owl:Class rdf:about="http://brancusi.usc.edu/bkms/BAMS.owl#BAMSC588">
<rdfs:label>Superior olivary complex periolivary region</rdfs:label>
<rdfs:subClassOf>
<owl:Restriction>
<owl:onProperty rdf:resource="http://brancusi.usc.edu/bkms/BAMS.owl#ro:has_part"/>
